.table-responsive-md
  table.table
    thead.thead-dark
      tr
        th(scope='col') #
        th(scope='col') First Name
        th(scope='col') Last Name
        th(scope='col') Email
    tbody
      tr(*ngFor='let user of users; let i = index')
        th(scope='row') {{i}}
        td {{user.first_name}}
        td {{user.last_name}}
        td {{user.email}}
